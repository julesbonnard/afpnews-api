{
  "name": "afpnews-api",
  "version": "2.2.0",
  "description": "Node helper functions to authenticate and fetch AFP Core API",
  "main": "dist/cjs/index.js",
  "type": "commonjs",
  "types": "./dist/cjs/index.d.cts",
  "module": "dist/esm/index.js",
  "unpkg": "./dist/bundles/apicore.min.js",
  "jsdelivr": "./dist/bundles/apicore.min.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "browser": {
        "script": "./dist/bundles/apicore.min.js",
        "import": {
          "types": "./dist/esm/index.d.mts",
          "default": "./dist/bundles/apicore.min.mjs"
        },
        "require": {
          "types": "./dist/cjs/index.d.cts",
          "default": "./dist/bundles/apicore.min.js"
        }
      },
      "import": {
        "types": "./dist/esm/index.d.mts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "source": "src/index.ts",
  "files": [
    "dist",
    "Readme.md",
    "LICENSE.md"
  ],
  "scripts": {
    "clean": "rimraf dist .rts2_cache_* src/grammar/index.ts",
    "build:parser": "nearleyc src/grammar/index.ne --out ./src/grammar/index.ts",
    "build": "npm-run-all -l clean build:parser && rollup -c && ./fixup",
    "postbuild": "rimraf dist/types",
    "build:watch": "nodemon -L -e ts,json,js --config ./nodemon.json --exec \"npm run build\"",
    "lint": "eslint .",
    "prepare": "npm run lint && npm run build"
  },
  "engines": {
    "node": ">= 12.20.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/julesbonnard/afpnews-api.git"
  },
  "keywords": [
    "AFP",
    "News",
    "API",
    "Node"
  ],
  "author": "Jules Bonnard",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/julesbonnard/afpnews-api/issues"
  },
  "homepage": "https://github.com/julesbonnard/afpnews-api#readme",
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "eslint": "^9.32.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^52.0.0",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "rollup": "^4.46.1",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  },
  "dependencies": {
    "@types/btoa-lite": "^1.0.2",
    "@types/moo": "^0.5.10",
    "@types/nearley": "^2.11.5",
    "@types/node": "^24.1.0",
    "@types/statuses": "^2.0.6",
    "btoa-lite": "^1.0.0",
    "events": "^3.3.0",
    "moo": "^0.5.2",
    "nearley": "^2.20.1",
    "statuses": "^2.0.2",
    "zod": "^4.0.13",
    "tslib": "^2.6.2"
  }
}
