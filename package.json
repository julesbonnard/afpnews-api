{
  "name": "afpnews-api",
  "version": "1.6.0",
  "description": "Node helper functions to authenticate and fetch AFP News API",
  "main": "dist/umd/afpnews-api.node.js",
  "module": "dist/esm/index.js",
  "types": "dist/typings/index.d.ts",
  "engines": {
    "node": ">= 6.0.0"
  },
  "scripts": {
    "clean": "rimraf dist stats.json",
    "dev": "webpack --mode development --watch --display-error-details",
    "build": "webpack --mode production",
    "prebuild": "tsc",
    "analyse": "webpack --mode production --json > stats.json && npx webpack-bundle-analyzer stats.json",
    "test": "mocha --require @babel/register --colors --exit --timeout 5000 ./test/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/julesbonnard/afpnews-api.git"
  },
  "keywords": [
    "AFP",
    "News",
    "API",
    "Node"
  ],
  "author": "Jules Bonnard",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/julesbonnard/afpnews-api/issues"
  },
  "homepage": "https://github.com/julesbonnard/afpnews-api#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "@types/btoa": "^1.2.2",
    "@types/node": "^11.9.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^6.2.0",
    "eslint": "^5.14.1",
    "eslint-config-recommended": "^4.0.0",
    "eslint-loader": "^2.1.2",
    "mocha": "^6.0.1",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.5",
    "webpack-bundle-analyzer": "^3.0.4",
    "webpack-cli": "^3.2.3",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "axios": "^0.18.0",
    "btoa": "^1.2.1",
    "form-data": "^2.3.3",
    "lucene": "^2.0.3"
  }
