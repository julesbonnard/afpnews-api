{
  "name": "afpnews-api",
  "version": "2.0.21",
  "description": "Node helper functions to authenticate and fetch AFP Core API",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "unpkg": "./dist/bundles/apicore.min.js",
  "jsdelivr": "./dist/bundles/apicore.min.js",
  "type": "commonjs",
  "exports": {
    "types": "./dist/types/index.d.ts",
    "browser": {
      "script": "./dist/bundles/apicore.min.js",
      "import": "./dist/bundles/apicore.min.mjs",
      "require": "./dist/bundles/apicore.min.js"
    },
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "source": "src/index.ts",
  "files": [
    "dist",
    "Readme.md",
    "LICENSE.md"
  ],
  "scripts": {
    "clean": "rimraf dist .rts2_cache_* src/grammar/index.ts",
    "build:parser": "nearleyc src/grammar/index.ne --out ./src/grammar/index.ts",
    "build:esm": "cross-env BABEL_ENV=esmUnbundled babel src --extensions '.ts' --out-dir 'dist/esm'",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --extensions '.ts' --out-dir 'dist/cjs'",
    "build:esmBundled": "cross-env BABEL_ENV=esmBundled rollup -c rollup.config.ts",
    "build:umdBundled": "cross-env BABEL_ENV=umdBundled rollup -c rollup.config.ts",
    "build:declarations": "tsc -p tsconfig.json --declaration --emitDeclarationOnly",
    "build": "npm-run-all -l clean build:parser -p build:esm build:cjs build:esmBundled build:umdBundled build:declarations && ./fixup",
    "build:watch": "nodemon -L -e ts,json,js --config ./nodemon.json --exec \"npm build\"",
    "lint": "eslint . --ext .ts",
    "prepare": "npm run lint && npm run build"
  },
  "engines": {
    "node": ">= 12.20.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/julesbonnard/afpnews-api.git"
  },
  "keywords": [
    "AFP",
    "News",
    "API",
    "Node"
  ],
  "author": "Jules Bonnard",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/julesbonnard/afpnews-api/issues"
  },
  "homepage": "https://github.com/julesbonnard/afpnews-api#readme",
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/eslint-plugin-tslint": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "babel-plugin-add-import-extension": "^1.6.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.0.4",
    "nodemon": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "rollup": "^2.79.1",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/btoa-lite": "^1.0.2",
    "@types/moo": "^0.5.9",
    "@types/nearley": "^2.11.5",
    "@types/node": "^20.11.16",
    "@types/statuses": "^2.0.4",
    "btoa-lite": "^1.0.0",
    "core-js": "^3.35.1",
    "cross-fetch": "4.0.0",
    "events": "^3.3.0",
    "form-data": "^4.0.0",
    "moo": "^0.5.2",
    "nearley": "^2.20.1",
    "statuses": "^2.0.1",
    "zod": "^3.22.4"
  }
}
