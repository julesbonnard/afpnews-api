{
  "name": "afpnews-api",
  "version": "1.5.0",
  "description": "Node helper functions to authenticate and fetch AFP News API",
  "main": "dist/afpnews-api.node.js",
  "module": "src/index.js",
  "engines": {
    "node": ">= 6.0.0"
  },
  "scripts": {
    "clean": "rimraf dist stats.json",
    "dev": "webpack --mode development --watch --display-error-details",
    "build": "webpack --mode production",
    "analyse": "webpack --mode production --json > stats.json && npx webpack-bundle-analyzer stats.json",
    "test": "nyc --reporter=html --reporter=text mocha --require @babel/register --colors --exit --timeout 5000 ./test/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/julesbonnard/afpnews-api.git"
  },
  "keywords": [
    "AFP",
    "News",
    "API",
    "Node"
  ],
  "author": "Jules Bonnard",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/julesbonnard/afpnews-api/issues"
  },
  "homepage": "https://github.com/julesbonnard/afpnews-api#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^5.2.0",
    "dotenv": "^6.0.0",
    "eslint": "^5.6.1",
    "eslint-config-recommended": "^4.0.0",
    "eslint-loader": "^2.1.1",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "rimraf": "^2.6.2",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "axios": "^0.18.0",
    "btoa": "^1.2.1",
    "form-data": "^2.3.2",
    "lucene-query-parser": "^1.2.0"
  }
}
